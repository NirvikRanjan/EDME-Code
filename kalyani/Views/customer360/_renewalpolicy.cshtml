    function allowAlphaNumericSpace(e) {
        var code = ('charCode' in e) ? e.charCode : e.keyCode;
        if (!(code > 47 && code < 58) && // numeric (0-9)
            !(code > 64 && code < 91) && // upper alpha (A-Z)
            !(code > 96 && code < 123)) { // lower alpha (a-z)
            //!(code == 32) && // space
            e.preventDefault();
        }
    }
    function editoptions(editfor) {
        var newinitialValue = parseFloat($('#txtIdv').val());
        if (editfor == "phone") {
            $("#txtrenewalphonenum").prop("readonly", false);
            $("#txtrenewalphonenum").focus();

        }
        if (editfor == "regno") {
            //$("#txtinsrenewalpolicynumber").prop("readonly", false);
            //$("#txtinsrenewalpolicynumber").focus();

            var regInput = $("#txtinsrenewalpolicynumber");
            var vehinum = $("#txtinsrenewalpolicynumber").val();

            // Check if the vehinum starts with a letter or number
            var startsWithLetter = /^[A-Za-z]{2}/.test(vehinum);
            var startsWithNumber = /^[0-9]{2}/.test(vehinum);

            // Initially set the readonly property to false and focus on the field
            regInput.prop("readonly", false);
            regInput.focus();

            // Listen for input changes
            regInput.on('input', function () {
                var regNo = $(this).val();

                // If starts with a letter, we need to validate the XX99XX9999 format
                if (startsWithLetter) {
                    var regNoPattern = /^[A-Za-z]{2}[0-9]{2}[A-Za-z]{2}[0-9]{4}$/;
                    if (!regNoPattern.test(regNo)) {
                        $(this).removeClass("valid").addClass("invalid");
                    } else {
                        $(this).removeClass("invalid").addClass("valid");
                    }
                }
                // If starts with a number, we need to validate the 99XX9999X format
                else if (startsWithNumber) {
                    var regNoPattern = /^[0-9]{2}[A-Za-z]{2}[0-9]{4}[A-Za-z]$/;
                    if (!regNoPattern.test(regNo)) {
                        $(this).removeClass("valid").addClass("invalid");
                    } else {
                        $(this).removeClass("invalid").addClass("valid");
                    }
                }
            });

            // Restrict user input to only valid characters based on starting number/letter
            regInput.on('keydown', function (event) {
                var regNo = $(this).val();
                var key = String.fromCharCode(event.which);

                // Allow backspace and delete keys
                if (event.key === "Backspace" || event.key === "Delete") {
                    return; // Allow the deletion of characters
                }

                // For cases where vehinum starts with two letters
                if (startsWithLetter) {
                    // The first two positions must be letters
                    if (regNo.length < 2) {
                        if (!/[A-Za-z]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                    // The next two positions must be numbers
                    else if (regNo.length < 4) {
                        if (!/[0-9]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                    // The next two positions must be letters
                    else if (regNo.length < 6) {
                        if (!/[A-Za-z]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                    // The last four positions must be numbers
                    else if (regNo.length < 10) {
                        if (!/[0-9]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                }
                // For cases where vehinum starts with two numbers
                else if (startsWithNumber) {
                    //if (regNo.length >= 9) {
                    //    event.preventDefault();  // Stop further input once length is 9
                    //    return;
                    //}
                    // The first two positions must be numbers
                    if (regNo.length < 2) {
                        if (!/[0-9]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                    // The next two positions must be letters
                    else if (regNo.length < 4) {
                        if (!/[A-Za-z]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                    // The next four positions must be numbers
                    else if (regNo.length < 8) {
                        if (!/[0-9]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                    // The last position must be a letter
                    else if (regNo.length < 9) {
                        if (!/[A-Za-z]/.test(key)) {
                            event.preventDefault();
                        }
                    }
                }
            });

        }
