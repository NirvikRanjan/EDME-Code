    function getMakeModelVariantFromAPI(ddlMakeId, ddlModelId, ddlVariantId) {
        $.ajax({
            type: 'GET',
            url: siteRoot + "/customer360/GetMakeModelVariantFromAPIAsync",
            dataType: 'json',
            success: (response) => {
                if (response.success == true) {
                    if (response.makeLists && response.makeLists.length > 0) {
                        $("#" + ddlMakeId).empty().append('<option value="">-- Select --</option>');

                        $.each(response.makeLists, function (index, value) {
                            $("#" + ddlMakeId).append(
                                $('<option></option>').val(value.FKMAKE_ID).html(value.Make_Name)
                            );
                        });

                        if (response.makeDefaultValue) {
                            $("#" + ddlMakeId).val(response.makeDefaultValue).trigger("change");
                        }
                        else {
                            $("#" + ddlMakeId).trigger("change");
                        }
                    } else {
                        Lobibox.notify('warning', {
                            continueDelayOnInactiveTab: true,
                            msg: 'Make data not Found for this Dealer and OEM.'
                        });
                    }

                    if (response.modelLists && response.modelLists.length > 0) {
                        $("#" + ddlModelId).empty().append('<option value="">-- Select --</option>');

                        $.each(response.modelLists, function (index, value) {
                            $("#" + ddlModelId).append(
                                $('<option></option>').val(value.FKMODEL_ID).html(value.MODEL_CODE)
                            );
                        });

                        if (response.modelDefaultValue) {
                            $("#" + ddlModelId).val(response.modelDefaultValue).trigger("change");
                        }
                        else {
                            $("#" + ddlModelId).trigger("change");
                        }
                    } else {
                        Lobibox.notify('warning', {
                            continueDelayOnInactiveTab: true,
                            msg: 'Model data not Found for this Dealer or OEM.'
                        });
                    }

                    if (response.variantLists && response.variantLists.length > 0) {
                        $("#" + ddlVariantId).empty().append('<option value="">-- Select --</option>');

                        $.each(response.variantLists, function (index, value) {
                            $("#" + ddlVariantId).append(
                                $('<option></option>').val(value.VARIANT_ID).html(value.VARIANT_CODE)
                            );
                        });

                        if (response.variantDefaultValues) {
                            $("#" + ddlVariantId).val(response.variantDefaultValues).trigger("change");
                        }
                        else {
                            $("#" + ddlVariantId).trigger("change");
                        }
                    } else {
                        Lobibox.notify('warning', {
                            continueDelayOnInactiveTab: true,
                            msg: 'Variant data not found for this Dealer and OEM.'
                        });
                    }

                    Lobibox.notify('success', {
                        continueDelayOnInactiveTab: true,
                        msg: response.message,
                        rounded: true,
                        closable: true
                    });
                }
                else {
                    Lobibox.notify('error', {
                        continueDelayOnInactiveTab: true,
                        msg: response.message,
                        rounded: true,
                        closable: true
                    });
                }
            },
            error: (xhr, status, error) => {
                Lobibox.notify('error', {
                    continueDelayOnInactiveTab: true,
                    msg: `An error occurred: ${xhr.status} - ${xhr.statusText}. Please try again later.`,
                    rounded: true,
                    closable: true
                });
            }
        });
    }

