<style>
    .red-tooltip + .tooltip > .tooltip-inner {
        background-color: wheat;
        border: 1px;
        color: black;
        min-width: 260px;
    }

    .red-tooltip + .tooltip > .tooltip-arrow {
        border-bottom-color: burlywood;
        border: 1px;
        color: black;
        min-width: 260px;
        font-weight: bold;
    }

    input[type=radio].turncheckbox {
        cursor: pointer;
        padding: 0.5em;
        -webkit-appearance: none;
        outline: 0.1em solid black;
        outline-offset: 0.1em;
    }

        input[type=radio].turncheckbox:checked {
            display: inline-block;
            background-color: forestgreen;
        }

    .turncheckbox:disabled {
        cursor: not-allowed !important;
        background-color: #e9ecef;
        border: 1px solid #ced4da;
        opacity: 0.6;
    }

    .inputcolorchange:focus {
        background-color: white;
    }

    :read-only {
    }

    /* ... so you might want to scope it */
    input[type=text]:read-only {
        cursor: not-allowed;
        background-color: red;
    }

    input[type=text]:focus {
        border-color: dodgerBlue;
    }

    .inputWithIcon input[type=text] {
        padding-left: 14px;
        padding-right: 34px;
    }

    .inputWithIcon {
        position: relative;
    }

        .inputWithIcon i {
            position: absolute;
            right: 8px;
            top: 8px;
            padding: 3px 8px;
            color: #aaa;
            transition: .3s;
        }

        .inputWithIcon input[type=text]:focus + i {
            color: dodgerBlue;
        }

        .inputWithIcon.inputIconBg i {
            background-color: #aaa;
            color: #fff;
            padding: 9px 4px;
            border-radius: 4px 0 0 4px;
        }

    .checkboxes {
        width: 30px;
        height: 20px;
    }

    .inner {
        display: flex;
        align-items: center;
    }

        .inner img {
            margin-right: 10px; /* Spacing between the image and text */
        }

    h3 {
        margin: 0; /* Remove default margin for a cleaner look */
    }

    .inset-ck {
        width: 16px;
        height: 16px;
        box-shadow: inset 3px 3px 6px rgba(0, 0, 0, 0.3), inset -3px -3px 6px rgba(255, 255, 255, 0.1), inset -3px 3px 6px rgba(0, 0, 0, 0.3), inset 3px -3px 6px rgba(255, 255, 255, 0.1) !important;
    }

        .inset-ck:disabled {
            width: 15px;
            height: 15px;
            opacity: 0.5;
            box-shadow: none;
            cursor: not-allowed;
            font-weight: normal;
        }

    .bold-text {
        font-weight: bold;
    }

    .nxt-blue-btn {
        background-color: #286090;
        border-color: #204d74;
        color: white;
        font-weight: bold;
        border-radius: 5px;
        box-shadow: 3px 3px 3px #cecece, -3px -3px 3px #fff !important;
        width: 75px;
        text-align: center;
    }

    .btn-back-red {
        font-weight: bold;
        border-radius: 5px;
        box-shadow: 3px 3px 3px #cecece, -3px -3px 3px #fff;
        width: 75px;
        border-color: #a52723;
        margin-right: 15px;
        text-align: center;
    }

    .nxt-blue-btn:hover {
        color: white;
        background-color: #243E6D;
    }

    .btn-renapis {
        border-color: #204d74;
        border-radius: 5px;
        opacity: 100%;
        font-weight: bold;
        background-color: #3586af;
        color: white;
    }

        .btn-renapis:hover {
            color: white;
            background-color: #243E6D;
        }

    .search-mvscp-btn {
        background-color: #F5F5F5;
        border-color: #EFF0F3;
        font-weight: bold;
        border-radius: 0px !important;
        outline: none;
        /*box-shadow: 3px 3px 3px #cecece, -3px -3px 3px #fff !important;*/
        text-align: center;
        margin-left: 2px;
    }

        .search-mvscp-btn:hover {
            /*background-color: #EFF0F3;*/
            background-color: darkgrey;
        }

    /*.radio-inline {
        display: flex;
        align-items: center;
    }*/
</style>
