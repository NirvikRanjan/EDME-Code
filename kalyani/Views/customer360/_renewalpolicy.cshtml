    function handleProposerTypeChange(el) {
        var proposerType = el.value;

        if (proposerType === 'I') {
            $('#mand-salutation, #mand-firstname, #mand-gender, #mand-dob').show();
        } else {
            $('#mand-salutation, #mand-firstname, #mand-gender, #mand-dob').hide();
        }

        if (proposerType === 'C') {
            $('#mand-compsalu, #mand-compname, #mand-disignation, #mand-doi').show();
        } else {
            $('#mand-compsalu, #mand-compname, #mand-disignation, #mand-doi').hide();
        }
    }

    function handleFuelTypeChange(el) {
        var fuelType = el.value;

        if (fuelType === 'Electric' || fuelType === 'electric') {
            $('#mand-batcapacity').show();
        } else {
            $('#mand-batcapacity').hide();
        }
    }

    function toggleRegFieldsBasedOnPolicyType() {
        var isPolicyTypeNewOrRenew = $("input[name='policyTypeStatus']:checked").val();
        if (isPolicyTypeNewOrRenew === "R") {
            $('#mand-regNo').show();
            $('#mand-regDate').show();
        } else {
            $('#mand-regNo').hide();
            $('#mand-regDate').hide();
        }
    }
