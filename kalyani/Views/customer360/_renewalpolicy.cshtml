                var initialValue = parseFloat($('#txtIdv').val());
                if (isNaN(initialValue)) {
                    console.error('Initial value is not available.');
                    return;
                }

                var disValue = initialValue * 0.05;
                var minValue = initialValue - disValue;
                var maxValue = initialValue + disValue;

                $('#txtIdv').data('minValue', minValue);
                $('#txtIdv').data('maxValue', maxValue);
            });

            $('#txtIdv').on('blur', function () {
                var curValue = parseFloat($('#txtIdv').val());

                if (isNaN(curValue)) {
                    $('#txtIdv').val(newinitialValue);
                    //console.log("newinitialValue : ", newinitialValue);
                    return;
                }

                var minValue = $('#txtIdv').data('minValue');
                var maxValue = $('#txtIdv').data('maxValue');

                //console.log("Current Value: " + curValue);
                //console.log("Min Value: " + minValue);
                //console.log("Max Value: " + maxValue);

                if (curValue < minValue || curValue > maxValue) {
                    Lobibox.notify('warning', {
                        continueDelayOnInactiveTab: true,
                        msg: 'The entered value must be within 5% of the original value (between ' + minValue + ' and ' + maxValue + ').'
                    });

                    $('#txtIdv').val(newinitialValue);
                    $('#txtIdv').css('border', '2px solid red');
                } else {
                    $('#txtIdv').css('border', '');
                }
            });



       }
