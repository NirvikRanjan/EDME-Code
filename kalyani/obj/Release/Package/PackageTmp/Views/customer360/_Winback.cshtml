        function idproofvalidations() {

        var adharRegexp = /^[2-9]{1}[0-9]{3}\s{1}[0-9]{4}\s{1}[0-9]{4}$/;
        var regex = new RegExp("[A-Z]{5}[0-9]{4}WinBackrenewaltypeinBackrenewaltypeA-Z]{1}");
        /*  var regex1 = ToUpperew RegEToUpperxp("/^[0-3][0-9]{7}$/");*/
        //var regex1 = new RegExp("^(([A-Z]{2}[0-9]{2})(Modelodelodel|([A-Z]{2}-[0-9]{2}))((19|20)[0-9][0-9])[0-9]{7}$");
        /* var regex2 = new RegExp("/^[2-9]{1}[0-9]{3}\s{1}[0-9]{4}\s{1}[0-9]{4}$/");*/
        var regex3 = new RegExp("^([A-Za-z]){1}([0-9]){7}$");
        var regex4 = /^([a-zA-Z]){3}([0-9]){7}?$/g;
        var regex5 = /^([a-zA-Z0-9]){8,12}\s*$/g;

        var txtBoxValue = $('#txtidproof').val();
        var idprooftype = $('#ddlidproof').val()

        //if (idprooftype == "LicenseNo" ) {
        //    if (!regex1.test(txtBoxValue))
        //    {

        //        Lobibox.notify('warning', {
        //            continueDelayOnInactiveTab: true,
        //            msg: 'invalid License Number Ex:HR-0619850034761.'
        //        });

        //        return false;
        //}

        //}
         if (idprooftype == "PANno")
        {

            if (!regex.test(txtBoxValue))
            {

                Lobibox.notify('warning', {
                    continueDelayOnInactiveTab: true,
                    msg: 'Invalid PAN Number Ex:ABCDE1234F.'
                });

                return false;
        }

        }
        else if (idprooftype == "PassportNo") {
            if (!regex3.test(txtBoxValue))
            {
                Lobibox.notify('warning', {
                    continueDelayOnInactiveTab: true,
                    msg: 'Invalid Passport Number Ex:A3065731.'
                });

                return false;
        }
        }
        else if (idprooftype == "VoterIDNo") {

            if (!regex4.test(txtBoxValue))
            {
                Lobibox.notify('warning', {
                    continueDelayOnInactiveTab: true,
                    msg: 'invalid Voter Id Number Ex:ABE1234566 or abe1234566.'
                });

                return false;
        }
        }
        else if (idprooftype == "RationCardNo") {

            if (!regex5.test(txtBoxValue))
            {

                Lobibox.notify('warning', {
                    continueDelayOnInactiveTab: true,
                    msg: 'Invalid RationCard Number Ex:fafaafaf8989.'
                });
                return false;
        }
        }
        else if (idprooftype == "AadharNo") {

            if (!adharRegexp.test(txtBoxValue))
            {
                Lobibox.notify('warning', {
                    continueDelayOnInactiveTab: true,
                    msg: 'invalid Aadhar Number Ex:3675 9834 6015.'
                });
                return false;
             }

         }
 }


