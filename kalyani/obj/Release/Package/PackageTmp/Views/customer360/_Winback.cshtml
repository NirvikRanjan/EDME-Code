    function getcityBystate(stateddlId, cityddlId) {
        var stateId = $('#' + stateddlId).val();
        var cityDetails = @Html.Raw(Json.Encode(Model.city_masters));
        var statecities = cityDetails.filter(m => m.fkState_Id == stateId);
        $('#' + cityddlId).empty();
        $('#' + cityddlId).append(new Option("--Select--",""));

        var dropdown = document.getElementById(cityddlId);

        for (var i = 0; i < statecities.length; i++) {

            dropdown[dropdown.length] = new Option(statecities[i].city_name, statecities[i].id);

        }

        getPincodesByState(stateddlId, "ddlpincodeOne");
        getRtosByState(stateIdrto, "ddlrto");

    }

    function getfinancierBystateandCity(stateddlId, cityddlId) {
        $('#ddlfinancer').removeClass('selectpicker');

        var stateId = $('#' + stateddlId).val();
         var cityId = $('#' + cityddlId).val();
         var financierDetails = @Html.Raw(Json.Encode(Model.map_cp_financiers));
         var filteredFinancier = financierDetails.filter(m => m.fkState_Id == stateId && m.fkCity_Id == cityId);


         $('#ddlfinancer').empty();
         $('#ddlfinancer').append(new Option("--Select--", ""));
        var dropdown = document.getElementById('ddlfinancer');

         for (var i = 0; i < filteredFinancier.length; i++) {

             dropdown[dropdown.length] = new Option(filteredFinancier[i].Name, filteredFinancier[i].Financer_Id);

        }
    }

    function getRtosByState(stateIdrto, rtos) {

        var stateIdrto = $('#' + stateIdrto).val();
        var rtos = @Html.Raw(JsonConvert.SerializeObject(Model.rtos));
                @*var pincode =@Html.Raw(Json.Encode(Model.pincodemasters));*@
        $('#' + rtos).empty();
        $('#' + rtos).append(new Option("--Select--", ""));

        var rtosFilterbyState = rtos.filter(x => x.fkState_Id == stateIdrto);


        const x1 = rtosFilterbyState.reduce(
            (accumulator, current) => {
                if (!accumulator.some(x => x.fkState_Id === current.fkState_Id)) {
                    accumulator.push(current)
                }
                return accumulator;
            }, []
        )
        var dropdown1 = document.getElementById(rtos);

        for (var i = 0; i < x1.length; i++) {
            var rtosZone = ((x1[i].Name).toString());
            dropdown1[dropdown1.length] = new Option(rtosZone, rtosZone);

        }


    }




    function getPincodesByState(stateId, pincodeId)
            {

                var stateId = $('#' + stateId).val();
            var pincode = @Html.Raw(JsonConvert.SerializeObject(Model.pincodemasters));
                @*var pincode =@Html.Raw(Json.Encode(Model.pincodemasters));*@
        $('#' + pincodeId).empty();
        $('#' + pincodeId).append(new Option( "--Select--",""));

        var pincodeFilterbyState = pincode.filter(x => x.fkState_Id == stateId);


        const x = pincodeFilterbyState.reduce(
            (accumulator, current) => {
                if (!accumulator.some(x => x.fkState_Id === current.fkState_Id)) {
                    accumulator.push(current)
                }
                return accumulator;
            }, []
        )

         var dropdown1 = document.getElementById(pincodeId);

        for (var i = 0; i < x.length; i++)
        {
            var pinZone = ((x[i].PIN_Code).toString()).slice(0,2);
            dropdown1[dropdown1.length] = new Option(pinZone, pinZone);

                }
            }

