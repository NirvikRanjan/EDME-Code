            valideffectiveStartDate.setFullYear(valideffectiveStartDate.getFullYear() + Number.parseInt(tenure));
            valideffectiveStartDate.setDate(valideffectiveStartDate.getDate() - 1);
            $('#txtexpirydate').val(valideffectiveStartDate.getDate() + "-" + (valideffectiveStartDate.getMonth() + 1) + "-" + valideffectiveStartDate.getFullYear());

        }
        else {


            effectiveDate = validpolicyStartDate.getDate() + "-" + (validpolicyStartDate.getMonth() + 1) + "-" + (validpolicyStartDate.getFullYear()+1);
            $('#txteffectivedate').val(effectiveDate)

            var valideffectiveStartDate = $.datepicker.parseDate('dd-mm-yy', effectiveDate);
            valideffectiveStartDate.setFullYear(valideffectiveStartDate.getFullYear() + Number.parseInt(tenure));
            valideffectiveStartDate.setDate(valideffectiveStartDate.getDate() - 1);

            $('#txtexpirydate').val(valideffectiveStartDate.getDate() + "-" + (valideffectiveStartDate.getMonth() + 1) + "-" + valideffectiveStartDate.getFullYear());


        }
