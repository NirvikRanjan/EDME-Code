        function checkeffectiveandexpiryDate() {

        var tenure = $('#ddlPreviousPolicy').val();
        var policystartDate = $('#txtpolicystartdate').val();

        var policyExpiryDate = $('#txtpolicyenddate').val();

        var validpolicyStartDate = $.datepicker.parseDate('dd-mm-yy', policystartDate);

        var validpolicyExpiryDate = $.datepicker.parseDate('dd-mm-yy', policyExpiryDate);


        var todaysDate = new Date();
        var effectiveDate;
        if (validpolicyExpiryDate < todaysDate)
        {
            effectiveDate = todaysDate.getDate() + "-" + (todaysDate.getMonth()+1) + "-" + todaysDate.getFullYear();
            $('#txteffectivedate').val(effectiveDate)

