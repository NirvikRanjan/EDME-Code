    function getrelationbyGender() {
        var Gender;

      var relations = @Html.Raw(Json.Encode(Model.nomineerelationsLists));
       var relation = '@Model.vehi.Nominee_Relationship.Trim()';


        if ( relation == '') {
            var SelectGender = $('input[name="winbackPolicy.nomineegender"]:checked').val();

            var RelationbyGender = relations.filter(m => m.gender == SelectGender);
            $('#ddlrelation').empty();
            var dropdown = document.getElementById('ddlrelation');

            for (var i = 0; i < RelationbyGender.length; i++) {

                dropdown[dropdown.length] = new Option(RelationbyGender[i].relationship, RelationbyGender[i].relationship);

            }
        }
        else
        {
            var relationgender = relations.filter(m => m.relationship == relation);

            if (relationgender != "" && relationgender != undefined) {
                Gender = relationgender[0].gender;
            }


            if (relation == "") {

                Gender = relations[1].gender;

            }
            var reltionfilters = relations.filter(m => m.gender == Gender);
            //NomineeRelation(relationgender[0].gender)
            $('#ddlrelation').empty();
            var dropdown = document.getElementById('ddlrelation');

            for (var i = 0; i < reltionfilters.length; i++) {

                dropdown[dropdown.length] = new Option(reltionfilters[i].relationship, reltionfilters[i].relationship);

            }

            $('#ddlrelation').val(relation);
            if (Gender == 'M') {
                $("#rdNomineegender").prop("checked", true);

            }
            else if (Gender == 'F' ) {
                $("#rdNomineeFemalegender").prop("checked", true);

            }
        }
    }



